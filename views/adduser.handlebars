<h1>
  Add User
</h1>

{{#if error}} <span>{{error}}</span> {{/if}}

<form method="POST" action="/api/user/adduser">
  <div class="form-group">
    <label>ID</label>
    <input name="id" placeholder="Add Id" class="form-control" autocomplete="on">
  </div>
  <div class="form-group">
    <label>First Name</label>
    <input name="firstName" placeholder="First Name" class="form-control">
  </div>
  <div class="form-group">
    <label>Last Name</label>
    <input name="lastName" placeholder="Second Name" class="form-control">
  </div>
  <div class="form-group">
    <label>Email</label>
    <input name="email" placeholder="Email" class="form-control">
  </div>
  <div class="form-group">
    <label>Phone</label>
    <input name="phone" placeholder="Phone" class="form-control">
  </div>
  <input type="submit" value="Add User" class="btn btn-primary">
</form>